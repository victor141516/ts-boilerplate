{
  "name": "ts-boilerplate",
  "version": "1.0.0",
  "main": "dist/index.js",
  "license": "UNLICENSED",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "start": "ts-node src/index.ts",
    "build": "tsc",
    "run-compiled": "node",
    "lint": "eslint src/**",
    "lint:tsc": "tsc --noEmit"
  },
  "importSort": {
    ".ts, .tsx": {
      "parser": "typescript",
      "style": "module-scoped"
    }
  },
  "eslintIgnore": [
    "/**/*.js"
  ],
  "dependencies": {
    "jsdom": "^19.0.0",
    "node-fetch": "^3.2.5"
  },
  "devDependencies": {
    "@types/jsdom": "^16.2.14",
    "@types/node": "^17.0.40",
    "@types/node-fetch": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "prettier": "^2.6.2",
    "ts-node": "^10.8.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.7.3"
  }
}